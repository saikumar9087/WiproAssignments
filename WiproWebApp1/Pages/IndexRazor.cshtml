@page
@model WiproWebApp1.Pages.IndexRazorModel
@{
}
<h1>@ViewData["Message"]</h1>
<form method="post">
	<label for="EmpId">EmployeeID:</label>
	<input type="number" id="EmpId" asp-for="emp.EmpId" />
	<br/>
	<label for="EmpName">Employee Name:</label>
	<input type="text" id="EmpName" asp-for="emp.EmpName" />
	<br/>

	<button type="submit">Save</button>

</form>
<h2>EMployee Details: </h2>
<p>Id: @Model.emp.EmpId</p>
<p>Name: @Model.emp.EmpName</p>

